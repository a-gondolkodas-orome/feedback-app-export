extends layout

block content
  a(href='/export/#{eventId}') Back

  h1= "Velemenyek"

  table
    thead
      tr
        th id
        th nev/jelige
        th valasz
        th timestamp
    tbody
      each answer, id in answers
        tr
          td= id
          td= answer["name"]
          td= answer["answer"]
          td= answer["timestamp"].toDate().toLocaleString()
